        Cell55.Text=&quot;&quot;;                   
      }
      else if (Email==&quot;0&quot;){
        Cell55.Text=&quot;&quot;;
      }
      if(Sms==&quot;1&quot;)  {
        Cell54.Text=&quot;&quot;;                   
      }
      else if (Sms==&quot;0&quot;){
        Cell54.Text=&quot;&quot;;
      }
    }
  }
}
