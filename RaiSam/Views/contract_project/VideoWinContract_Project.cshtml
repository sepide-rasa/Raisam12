@using Ext.Net.MVC
@using Hogaf.ExtNet.UX
@using Ext.Net;
@{
    var X = Html.X();
}

@using Ext.Net.MVC;

@(
 Html.X().Window().Listeners(l => l.BeforeClose.Handler = "CloseVideocontract_project()")
             .Height(565)
         .Width(1000)
                        .ID("Videocontract_project")
            .Closable(true)
                .Title("راهنمای تصویری پروژه هاو قراردادها")
        .AutoScroll(true)
        .Closable(true)
            .Maximizable(true)
        .Content(@<video width="100%;" id="AmozeshVideocontract_project" controls="true" autoplay="true" controls><source src='@Url.Action("DownloadFileVideoContract_Project", "contract_project")' type="video/mp4"></video>)

)
<script type="text/javascript">
    function CloseVideocontract_project() {
        $('#AmozeshVideocontract_project')[0].pause();
    }

</script>


