<script type="text/javascript">
    var fileId = 0;
    var LogoFileId = 0;
    var username = "";
    var password = "";
    var CodeMojavez = "";

    function EstelamMojavez() {
        App.ShowInfo_CompanyProfile2.body.mask('در حال بارگذاری...');
        Ext.net.DirectMethod.request({
            url: '/CompanyProfile/EstelamMojavezWin',
            params: {
                Code: CodeMojavez
            },
            success: function (data) {
                App.ShowInfo_CompanyProfile2.body.mask().hide();
            }
        });
    }
    function LoadDetailCompanyProfile2() {
        if ('@ViewBag.state' == "2") {
            App.ShowInfo_CompanyProfile2.queryById("btnSaveCompanyPersonal").hide(true);
            App.ShowInfo_CompanyProfile2.queryById("btnBackCompanyPersonal").hide(true);
        }
        else {
            App.ShowInfo_CompanyProfile2.queryById("btnSaveCompanyPersonal").show(true);
            App.ShowInfo_CompanyProfile2.queryById("btnBackCompanyPersonal").show(true);
        }

        Ext.net.DirectMethod.request({
            url: '/MonitoringRegisterComplete/DetailsCompanyProfile',
            params: {
                Id: '@ViewBag.CompanyProfileId',
                Type:'@ViewBag.PersonType'
            },
            success: function (result) {
              
                    App.ShowInfo_CompanyProfile2.queryById("CodeMojavez").hide();
                    App.ShowInfo_CompanyProfile2.queryById("lblCodeMojavez").hide();
                    App.ShowInfo_CompanyProfile2.queryById("btnEstelamMojavez").hide();
                    App.ShowInfo_CompanyProfile2.queryById("lblDl").setText("دانلود فایل اساسنامه:");
                    App.ShowInfo_CompanyProfile2.setTitle("نمایش اطلاعات ثبت نام کننده حقوقی");
                    App.ShowInfo_CompanyProfile2.queryById("PerposeCompanyProfile2").setText(result.fldGroupName);
                    App.ShowInfo_CompanyProfile2.queryById("FirstRegisterId").setValue(result.fldFirstRegisterId);
                    App.ShowInfo_CompanyProfile2.queryById("NameCompanyProfile2").setText(result.fldName);
                    App.ShowInfo_CompanyProfile2.queryById("LastNameCompanyProfile2").setText(result.fldFamily);
                    App.ShowInfo_CompanyProfile2.queryById("NameMCompanyProfile2").setText(result.fldNameModirAmel);
                    App.ShowInfo_CompanyProfile2.queryById("TelNamayande").setText(result.TelNamayande);
                    App.ShowInfo_CompanyProfile2.queryById("EmailNamayande").setText(result.EmailNamayande);
                    App.ShowInfo_CompanyProfile2.queryById("LastNameMCompanyProfile2").setText(result.fldFamilyModirAmel);
                    App.ShowInfo_CompanyProfile2.queryById("ShenaseMelliCompanyProfile2").setText(result.fldNationalCode);
                    App.ShowInfo_CompanyProfile2.queryById("FullNameCompanyProfile2").setText(result.fldFullName);
                    App.ShowInfo_CompanyProfile2.queryById("NickNameCompanyProfile2").setText(result.fldNickName);
                    App.ShowInfo_CompanyProfile2.queryById("DateTasisCompanyProfile2").setText(result.fldDateTasis);
                    App.ShowInfo_CompanyProfile2.queryById("Sh_SabtCompanyProfile2").setText(result.fldSh_Sabt);
                    App.ShowInfo_CompanyProfile2.queryById("Date_SabtCompanyProfile2").setText(result.fldDateSabt);
                    App.ShowInfo_CompanyProfile2.queryById("NoeSabtiCompanyProfile2").setText(result.fldTypeCompanyName);
                    App.ShowInfo_CompanyProfile2.queryById("MahalSabtCompanyProfile2").setText(result.fldCountryName + " - " + result.fldStateName + " - " + result.fldCityName);
                    App.ShowInfo_CompanyProfile2.queryById("CodeEghtesadiCompanyProfile2").setText(result.fldCodeEghtesadi);
                    App.ShowInfo_CompanyProfile2.queryById("CodePostiCompanyProfile2").setText(result.fldCodePosti);
                    App.ShowInfo_CompanyProfile2.queryById("TelephonCompanyProfile2").setText(result.fldPishShomare_Tel + "-" + result.fldTelphon);
                    App.ShowInfo_CompanyProfile2.queryById("MobileCompanyProfile2").setText(result.fldMobileModir);
                    App.ShowInfo_CompanyProfile2.queryById("FaxCompanyProfile2").setText(result.fldPishShomare_Namabar + "-" + result.fldNamabar);
                    App.ShowInfo_CompanyProfile2.queryById("MahalCompanyProfile2").setText(result.fldCountryName + " - " + result.fldStateName + " - " + result.fldCityName);
                    App.ShowInfo_CompanyProfile2.queryById("AddressCompanyProfile2").setText(result.fldAddress);
                    App.ShowInfo_CompanyProfile2.queryById("Email1CompanyProfile2").setText(result.fldEmail1);
                    App.ShowInfo_CompanyProfile2.queryById("TotalCountCompanyProfile2").setText(result.fldTedadKolSaham);
                    App.ShowInfo_CompanyProfile2.queryById("MablaghCompanyProfile2").setText(result.fldMablaghSaham.toLocaleString());
                    fileId = result.FileId;
                    LogoFileId = result.LogoFileId;
                    username = result.Username;
                    password = result.Password;

                    if (result.LogoFileId != null) {
                        var antiCachePart = (new Date()).getTime();
                        var url = '/MonitoringRegisterComplete/ShowLogo';
                        var g = url + '?dc=' + antiCachePart + '&FileId=' + result.LogoFileId;
                        var k = App.ShowInfo_CompanyProfile2.queryById('Image2');
                        k.setImageUrl(g);
                    }

                    if (result.fldAddressWebSite != "") {
                        App.ShowInfo_CompanyProfile2.queryById("WebAddressCompanyProfile2").setText(result.fldAddressWebSite);
                    }
                    else {
                        App.ShowInfo_CompanyProfile2.queryById("WebAddressCompanyProfile2").setText("ندارد");
                    }
                    if (result.fldEmail2 != "") {
                        App.ShowInfo_CompanyProfile2.queryById("Email1CompanyProfile2").setHeight(32);
                        App.ShowInfo_CompanyProfile2.queryById("fieldsetOtherEmail").show();
                        App.ShowInfo_CompanyProfile2.queryById("Email2CompanyProfile2").show();
                        App.ShowInfo_CompanyProfile2.queryById("lblEmail2").show();
                        App.ShowInfo_CompanyProfile2.queryById("Email2CompanyProfile2").setText(result.fldEmail2);
                    }
                    if (result.fldEmail3 != "") {
                        if (result.fldEmail2 == "") {
                            App.ShowInfo_CompanyProfile2.queryById("lblEmail3").getEl().dom.style = "padding:0;";
                        }
                        App.ShowInfo_CompanyProfile2.queryById("Email1CompanyProfile2").setHeight(32);
                        App.ShowInfo_CompanyProfile2.queryById("fieldsetOtherEmail").show();
                        App.ShowInfo_CompanyProfile2.queryById("Email3CompanyProfile2").show();
                        App.ShowInfo_CompanyProfile2.queryById("lblEmail3").show();
                        App.ShowInfo_CompanyProfile2.queryById("Email3CompanyProfile2").setText(result.fldEmail3);
                    }
                
                Ext.net.Mask.hide();
            }
        });
    }

