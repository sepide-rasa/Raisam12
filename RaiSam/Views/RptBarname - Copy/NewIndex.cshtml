    var addKeyMapRptBarname = function (cmp) {
        var CboTypeRPT = App.RptBarname.queryById('CboTypeRPT');
        var CboTypeBaze = App.RptBarname.queryById('CboTypeBaze');

        this.keyMap = new Ext.util.KeyMap({
            target: cmp.getEl(),
            binding: [{
                key: [13, 9], /*Ext.EventObject.ENTER,*/
                fn: function (key, e) {
                    if (e.getKey() == 9) {
                        e.stopEvent();
                    }
                    var keyfield = App.RptBarname.queryById(Ext.getCmp((Ext.Element.getActiveElement()).name).itemId);
                    var nextfld;
                    
                    if (keyfield.itemId == "CboTypeBaze") {
                        if (CboTypeBaze.getValue() == "0")
                            nextfld = App.RptBarname.queryById('txtAzTarikh');
                        else if (CboTypeRPT.getValue() == "1") {
                            alert();
                            nextfld = App.RptBarname.queryById('CboAzMonth');
                        }
                    }
                    if (keyfield.itemId == "txtTaTarikh") {
                        if (CboTypeRPT.getValue() == "0") 
                            nextfld = App.RptBarname.queryById('btnPrint');
                        else if (CboTypeRPT.getValue() == "1")
                            nextfld = App.RptBarname.queryById('btnPrint');
                        else if (CboTypeRPT.getValue() == "2")
                            nextfld = App.RptBarname.queryById('btnPrint');
                        else if (CboTypeRPT.getValue() == "3")
                            nextfld = App.RptBarname.queryById('txtAzShVagon');
                        else if (CboTypeRPT.getValue() == "4")
                            nextfld = App.RptBarname.queryById('btnUp');
                    }
                    if (keyfield.itemId == "CboTaYear") {
                        if (CboTypeRPT.getValue() == "0")
                            nextfld = App.RptBarname.queryById('btnPrint');
                        else if (CboTypeRPT.getValue() == "1")
                            nextfld = App.RptBarname.queryById('btnPrint');
                        else if (CboTypeRPT.getValue() == "2")
                            nextfld = App.RptBarname.queryById('btnPrint');
                        else if (CboTypeRPT.getValue() == "3")
                            nextfld = App.RptBarname.queryById('txtAzShVagon');
                        else if (CboTypeRPT.getValue() == "4")
                            nextfld = App.RptBarname.queryById('btnUp');
                    }
                    else {
                        nextfld = keyfield.nextSibling().nextSibling();
                    }
                    nextfld.focus();
                    nextfld.selectText();
                }
            }]
        });
    }
